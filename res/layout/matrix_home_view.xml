    </com.word.wordinsidehome.MyRelativeLayout>

    <com.word.wordinsidehome.MyRelativeLayout
        android:id="@+id/matrix_home_layout_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/matrix_home_layout_2"
        android:layout_below="@+id/matrix_home_layout_2"
        android:layout_marginLeft="-62.0dip"
        android:layout_marginTop="-62.0dip"
        android:layout_toRightOf="@+id/matrix_home_layout_3"
        android:background="@drawable/select_focus_bg"
        android:focusable="true">

        <ViewFlipper
            android:id="@+id/matrix_home_flipper_4"
            android:layout_width="380.0dip"
