            android:background="#99000000"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:textColor="@color/white"
            android:textSize="24.0sp"
            android:visibility="gone"/>
    </com.word.wordinsidehome.MyRelativeLayout>

    <com.word.wordinsidehome.MyRelativeLayout
        android:id="@+id/matrix_home_layout_2"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_alignBottom="@+id/matrix_home_layout_1"
        android:layout_alignTop="@+id/matrix_home_layout_1"
        android:layout_marginLeft="-62.0dip"
        android:layout_toRightOf="@+id/matrix_home_layout_1"
        android:background="@drawable/select_focus_bg"
        android:focusable="true">

        <ViewFlipper
            android:id="@+id/matrix_home_flipper_2"
