<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

    <com.word.wordinsidehome.MyRelativeLayout
        android:id="@+id/matrix_home_layout_0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="50.0dip"
        android:layout_marginTop="21.0dip"
        android:background="@drawable/select_focus_bg"
        android:focusable="true">

        <ViewFlipper
            android:id="@+id/matrix_home_flipper_0"
            android:layout_width="309.0dip"
            android:layout_height="468.0dip"
            android:background="@drawable/matrix_home_flipper_0_bg"/>

        <TextView
            android:id="@+id/matrix_home_flipper_0_text_view"
            android:layout_width="wrap_content"
            android:layout_height="37.0dip"
            android:layout_alignBottom="@+id/matrix_home_flipper_0"
            android:layout_alignLeft="@+id/matrix_home_flipper_0"
            android:layout_alignRight="@+id/matrix_home_flipper_0"
            android:background="#99000000"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text="@string/app_name"
            android:textColor="@color/white"
            android:textSize="24.0sp"
            android:visibility="gone"/>
    </com.word.wordinsidehome.MyRelativeLayout>

    <com.word.wordinsidehome.MyRelativeLayout
        android:id="@+id/matrix_home_layout_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="-62.0dip"
        android:layout_marginTop="21.0dip"
        android:layout_toRightOf="@+id/matrix_home_layout_0"
        android:background="@drawable/select_focus_bg"
