            android:singleLine="true"
            android:textColor="@color/white"
            android:textSize="24.0sp"
            android:visibility="gone"/>
    </com.word.wordinsidehome.MyRelativeLayout>

    <com.word.wordinsidehome.MyRelativeLayout
        android:id="@+id/matrix_home_layout_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/matrix_home_layout_1"
        android:layout_below="@+id/matrix_home_layout_1"
        android:layout_marginTop="-62.0dip"
        android:layout_toRightOf="@+id/matrix_home_layout_0"
        android:background="@drawable/select_focus_bg"
        android:focusable="true">

        <ViewFlipper
            android:id="@+id/matrix_home_flipper_3"
            android:layout_width="380.0dip"
            android:layout_height="223.0dip"
            android:background="@drawable/matrix_home_flipper_3_bg"/>

